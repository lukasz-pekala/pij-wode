{
  "name": "pij-wode",
  "version": "1.0.0",
  "description": "simple app to learn React, TypeScript and other front-emd related technologies",
  "default": "index.html",
  "scripts": {
    "clean:yarn-cache": "yarn cache clean",
    "clean:node_modules": "rmdir node_modules /s",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukasz-pekala/pij-wode.git"
  },
  "keywords": [
    "react",
    "redux",
    "TypeScript",
    "mui",
    "anf",
    "architektura",
    "na",
    "froncie"
  ],
  "author": "Łukasz Pękala, Weronika Kurek-Pękala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lukasz-pekala/pij-wode/issues"
  },
  "homepage": "https://github.com/lukasz-pekala/pij-wode#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@testing-library/react": "^13.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "jest": {
    "reporters": [
      "default",
      "github-actions"
    ]
  }
}
